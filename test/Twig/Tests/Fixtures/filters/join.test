--TEST--
"join" filter
--TEMPLATE--
{{ ["foo", "bar"]|join(', ') }}
{{ foo|join(', ') }}
{{ bar|join(', ') }}
{{ ["a", "<br />"]|join(', ') }}
{{ safe|join(', ') }}
--DATA--
return array('foo' => new TwigTestFoo(), 'bar' => new ArrayObject(array(3, 4)), 'safe' => array(new Twig_Markup('a', 'UTF-8'), new Twig_Markup('<br />', 'UTF-8')))
--EXPECT--
foo, bar
1, 2
3, 4
a, &lt;br /&gt;
a, <br />
